{
	"name": "@marsidev/react-turnstile",
	"version": "0.1.0",
	"packageManager": "pnpm@7.29.0",
	"description": "Cloudflare Turnstile integration for React.",
	"author": "Luis Marsiglia <marsiglia.business@gmail.com>",
	"license": "MIT",
	"type": "module",
	"private": false,
	"homepage": "https://github.com/marsidev/react-turnstile",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/marsidev/react-turnstile.git"
	},
	"bugs": "https://github.com/marsidev/react-turnstile/issues",
	"exports": {
		".": {
			"types": "./dist/index.d.ts",
			"require": "./dist/index.cjs",
			"import": "./dist/index.mjs"
		}
	},
	"main": "./dist/index.mjs",
	"module": "./dist/index.mjs",
	"types": "./dist/index.d.ts",
	"files": [
		"dist"
	],
	"scripts": {
		"build": "unbuild",
		"build:watch": "nodemon --ext .ts,.tsx --watch src --exec unbuild",
		"stub": "unbuild --stub",
		"lint": "eslint .",
		"lint:fix": "eslint . --fix",
		"prepublishOnly": "pnpm run build",
		"release": "bumpp && npm publish",
		"type-check": "tsc --noEmit"
	},
	"peerDependencies": {
		"react": ">=16.8.0",
		"react-dom": ">=16.8.0"
	},
	"devDependencies": {
		"bumpp": "9.0.0",
		"nodemon": "2.0.21",
		"unbuild": "0.8.9"
	},
	"keywords": [
		"cloudflare-turnstile",
		"turnstile",
		"cloudflare",
		"react"
	],
	"sideEffects": false,
	"pnpm": {
		"peerDependencyRules": {
			"ignoreMissing": []
		}
	},
	"publishConfig": {
		"access": "public"
	},
	"eslintConfig": {
		"extends": "custom"
	}
}
